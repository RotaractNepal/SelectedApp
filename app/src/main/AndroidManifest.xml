<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="np.com.rotaractnepalapp.rotaract">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_rotaract_nepal_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_rotaract_nepal_app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Chat"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.UserProfileActivity"
            android:parentActivityName=".Activity.Chat" />
        <activity
            android:name=".Activity.ConversationActivity"
            android:parentActivityName=".Activity.Chat" />
        <activity android:name=".Activity.News" />
        <activity android:name=".Activity.Events" />
        <activity android:name=".Activity.ClubActivity.ClubVIReview" />
        <activity android:name=".Activity.Club" />
        <activity android:name=".Activity.ClubActivity.ClubIReview" />
        <activity android:name=".Activity.ClubActivity.ClubIIReview" />
        <activity android:name=".Activity.ClubActivity.ClubIIIReview" />
        <activity android:name=".Activity.ClubActivity.ClubIVReview" />
        <activity android:name=".Activity.ClubActivity.ClubVReview" />
        <activity android:name=".Activity.ClubActivity.ClubVIIReview" />
        <activity android:name=".Activity.ClubActivity.ClubVIIIReview" />
        <activity android:name=".Activity.ClubActivity.ClubIXReview" />
        <activity android:name=".Activity.ClubActivity.ClubXReview" />
        <activity android:name=".Activity.ClubActivity.ClubXIReview" />
        <activity android:name=".Activity.ClubActivity.ClubXIIReview" />
        <activity android:name=".Activity.ClubActivity.ClubXIIIReview" />
        <activity android:name=".Activity.AboutUs" />
        <activity android:name=".Activity.BloodSearch" />
        <activity
            android:name=".Activity.Profile"
            android:parentActivityName=".Activity.Chat" />
        <activity
            android:name=".Activity.loginActivity"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.SignUpActivity"
            android:parentActivityName=".Activity.loginActivity" />

        <service android:name=".Services.DeviceTokenService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".Services.MyAppNotificationService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".ClubEvents"></activity>
    </application>

</manifest>